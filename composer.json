{
    "name": "rubix/tensor",
    "type": "library",
    "description": "Tensor is a basic linear algebra subprograms implementation in PHP.",
    "homepage": "https://github.com/RubixML/Tensor",
    "license": "MIT",
    "keywords": [
        "linear", "algebra", "php"
    ],
    "authors": [
        {
            "name": "Andrew DalPino",
            "email": "me@andrewdalpino.com"
        }
    ],
    "require": {
        "php": ">=7.1.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10.0",
        "phpstan/phpstan-phpunit": "^0.10.0",
        "phpunit/phpunit": "7.2.*"
    },
    "autoload": {
        "psr-4": {
            "Rubix\\Tensor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rubix\\Tensor\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyze": "phpstan analyse -c phpstan.neon",
        "test": "phpunit"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
